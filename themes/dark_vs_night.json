{
    "$schema": "vscode://schemas/color-theme",
    "name": "Dark (Visual Studio) - Adjusted for Night Light",
    "colors": {
        "checkbox.border": "#737373",
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4",
        "editor.inactiveSelectionBackground": "#404448",
        "editorIndentGuide.background1": "#464646",
        "editorIndentGuide.activeBackground1": "#7A7A7A",
        "editor.selectionHighlightBackground": "#B0DAFF33",
        "list.dropBackground": "#404448",
        "activityBarBadge.background": "#0082E6",
        "sideBarTitle.foreground": "#C1C1C1",
        "input.placeholderForeground": "#B0B0B0",
        "menu.background": "#292929",
        "menu.foreground": "#D0D0D0",
        "menu.separatorBackground": "#4F4F4F",
        "menu.border": "#4F4F4F",
        "statusBarItem.remoteForeground": "#FFFFFF",
        "statusBarItem.remoteBackground": "#1B936E",
        "ports.iconRunningProcessForeground": "#41A041",
        "sideBarSectionHeader.background": "#00000000",
        "sideBarSectionHeader.border": "#CCCCCC",
        "tab.selectedBackground": "#242424",
        "tab.selectedForeground": "#FFFFFFB3",
        "tab.lastPinnedBorder": "#CCCCCC",
        "list.activeSelectionIconForeground": "#FFFFFF",
        "terminal.inactiveSelectionBackground": "#404448",
        "widget.border": "#353537",
        "actionBar.toggledBackground": "#404450"
    },
    "tokenColors": [
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000088"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#73A966"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#5CDB9C"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#BDD7B2"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#75A675"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#DDBA88"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#A3FFD9"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "source.css entity.other.attribute-name.class",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.parent.less",
                "source.css entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#DDBA88"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#FF4F4F"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#BDD7B2"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#D29280"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#5F7AAB"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#72F388"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#D29280"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#888888"
            }
        },
        {
            "scope": [
                "meta.preprocessor",
                "entity.name.function.preprocessor"
            ],
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#D29280"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#BDD7B2"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#A3FFD9"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "keyword.operator.noexcept"
            ],
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": [
                "string",
                "meta.embedded.assembly"
            ],
            "settings": {
                "foreground": "#D29385"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#D29385"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#D29385"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#D57C7C"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "source.css variable",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#A3FFD9"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python"
            ],
            "settings": {
                "foreground": "#5CD85C"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#BDD7B2"
            }
        },
        {
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#A3FFD9"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#BDD7B2"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "foreground": "#5CD85C"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "newOperator": "#D4D4D4",
        "stringLiteral": "#D29385",
        "customLiteral": "#D4D4D4",
        "numberLiteral": "#BDD7B2"
    }
}
